{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/signalr",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "library": "vue@latest",
      "destination": "./wwwroot/js/vue",
      "files": [
        "dist/vue.*"
      ]
    },
    {
      "library": "moment@latest",
      "destination": "./wwwroot/js/moment",
      "files": [
        "min/moment*.js"
      ]
    }
  ]
}